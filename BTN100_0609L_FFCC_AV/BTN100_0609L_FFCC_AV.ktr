	FECHA_ALTA = new Date();
}
else{
    FECHA_ALTA = new Date(aux[1], aux[2]-1, aux[3]);
    &#47;&#47;FECHA_ALTA = date2str(FECHA_ALTA, &quot;yyyy-MM-dd&quot;);
}

&#47;&#47;ESTADO
if(ESTADO == &quot;01&quot;){
	ESTADO = &quot;en uso&quot;;
}
else if(ESTADO == &quot;02&quot;){
